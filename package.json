{
  "name": "proto-watch",
  "version": "1.1.0",
  "description": "Scaffoling for a proto watch app",
  "author": "LevelUp",
  "repository": {
    "type": "git",
    "url": "https://github.com/twlevelup/watch_edition"
  },
  "scripts": {
    "start": "webpack-dev-server --host 0.0.0.0 --inline --hot",
    "build": "webpack",
    "test": "karma start",
    "preci": "npm -s run jscs && npm -s run jshint",
    "ci": "npm -s run test",
    "todos": "fixme -p client/src/js && fixme -p client/spec/",
    "jscs": "jscs --fix client/src/js/ client/spec/",
    "jshint": "jshint --reporter=node_modules/jshint-stylish client/src/js client/spec/"
  },
  "dependencies": {
    "backbone": "^1.1.2",
    "css-loader": "^0.23.1",
    "file-loader": "^0.8.5",
    "handlebars": "^4.0.5",
    "handlebars-template-loader": "^0.6.0",
    "html-webpack-plugin": "^2.17.0",
    "http-server": "^0.9.0",
    "istanbul-instrumenter-loader": "^0.2.0",
    "jquery": "^2.1.3",
    "node-sass": "^3.3.3",
    "sass-loader": "^3.0.0",
    "style-loader": "^0.13.0",
    "underscore": "^1.8.3",
    "url-loader": "^0.5.7",
    "json-loader": "^0.5.4",
    "watch_framework": "git://github.com/twlevelup/watch_framework.git#v1.0",
    "webpack": "^1.12.12"
  },
  "engines": {
    "node": "0.12.x"
  },
  "devDependencies": {
    "docker-compose": "^0.1.0",
    "fixme": "^0.4.0",
    "jasmine-core": "^2.2.0",
    "jscs": "^3.0.4",
    "jshint": "^2.9.2",
    "jshint-stylish": "^2.1.0",
    "karma": "^0.13.21",
    "karma-coverage": "^0.5.3",
    "karma-jasmine": "^0.3.5",
    "karma-jasmine-jquery": "^0.1.1",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.6",
    "karma-spec-reporter": "0.0.24",
    "karma-threshold-reporter": "^0.1.15",
    "karma-webpack": "^1.7.0",
    "phantomjs-prebuilt": "^2.1.4",
    "webpack-dev-server": "^1.12.1",
    "webpack-livereload-plugin": "^0.8.2"
  }
}
